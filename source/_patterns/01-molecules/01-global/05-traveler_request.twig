<div class="searchbox">
    <div class="col-md-3">
        <div class="row">
            <div class="col-md-10">
                <img src="../../assets/images/{{travelers[0].image}}" alt="..." class="img-rounded img-responsive" />
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="searchbox-name"><strong>Visiting: </strong>Dubai, United Arab Emirates</div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="searchbox-name"><strong>Language: </strong>Swedish</div>
            </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            <div class="searchbox-name">
                <span class="glyphicon glyphicon-calendar">11/20/2015</span>
            </div>
          </div>
        </div>
        <div class="row request-list-action-button">
          <div class="col-md-12">
            {% if isPlanForm %}
            {% else %}
              <a href="{{ plinsert.link(link["pages-plan"]) }}" type="button" class="btn btn-primary btn-lg">Accept request</a>
            {% endif %}
          </div>
        </div>
    </div>
    <div class="col-md-9">
        <div class="row">
          <div class="col-md-9">
            <div class="searchbox-short-description">{{travelers[0].name}}</div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            <div class="searchbox-name"><strong>From: </strong>Sweden</div>
          </div>
        </div>
        <div class="row star-rating">
          <div class="col-md-3">
            <img src="../../assets/images/star.png" class="rating"></img>
            <img src="../../assets/images/star.png" class="rating"></img>
            <img src="../../assets/images/star.png" class="rating"></img>
          </div>
        </div>
        <div class="row">
          <div class="col-md-9">
            <ul class="interests-wrapper">
              <li><img src="../../assets/images/food.png" class="interest"></img></li>
              <li><img src="../../assets/images/film.png" class="interest"></img></li>
              <li><img src="../../assets/images/wine.png" class="interest"></img></li>
            </ul>
          </div>
        </div>
        <div class="row">
          <div class="col-md-9">
            <div class="searchbox-name">{{travelers[0].request}}</div>
          </div>
        </div>
    </div>
</div> 