<div class="searchbox">
    <div class="col-md-3">
        <div class="row">
            <div class="col-md-10">
                <img src="../../assets/images/{{travelers[1].image}}" alt="..." class="img-rounded img-responsive" />
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="searchbox-name"><strong>Visiting: </strong>Sweden</div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="searchbox-name"><strong>Language: </strong>Swedish</div>
            </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            <div class="searchbox-name">
                <span class="glyphicon glyphicon-calendar">11/20/2015</span>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            {% if isPlanForm %}
            {% else %}
              <button type="button" class="btn btn-primary btn-lg">Make an offer!</button>
            {% endif %}
          </div>
        </div>
    </div>
    <div class="col-md-9">
        <div class="row">
          <div class="col-md-9">
            <div class="searchbox-short-description">{{travelers[1].name}}</div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            <div class="searchbox-name"><strong>From: </strong>Sweden</div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-3">
            <img src="../../assets/images/star.png" class="rating"></img>
            <img src="../../assets/images/star.png" class="rating"></img>
            <img src="../../assets/images/star.png" class="rating"></img>
          </div>
        </div>
        <div class="row">
          <div class="col-md-9">
            <ul class="interests-wrapper">
              <li><img src="../../assets/images/food.png" class="interest"></img></li>
              <li><img src="../../assets/images/film.png" class="interest"></img></li>
              <li><img src="../../assets/images/wine.png" class="interest"></img></li>
            </ul>
          </div>
        </div>
        <div class="row">
          <div class="{% if isPlanForm %}col-md-12{% else %}col-md-9{% endif %}">
            <div class="searchbox-name">{{travelers[1].request}}</div>
          </div>
        </div>
    </div>
</div> 